<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Jela svijeta{% endblock %}</title>
    {% block stylesheets %}
        <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Spartan&display=swap">
        <link href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" type="text/css" rel="stylesheet">
        <link href="/css/style.css" rel="stylesheet" media="all" />
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.12.1/css/all.min.css" integrity="sha256-mmgLkCYLUQbXn0B1SRqzHar6dCnv9oZFPEC1g1cwlkk=" crossorigin="anonymous" />
    {% endblock %}
</head>
<body>
<div id="wrapper">
    <nav class="navbar navbar-inverse navbar-fixed-top col-lg-10 center-block">
        <div class="container-fluid">
            {% if app.request.attributes.get("_route") != 'app_homepage' and app.request.attributes.get("_route") != 'app_category' %}
                <a href="{% block back %}{{ path('app_homepage') }}{% endblock %}">
                    <span style="font-size: 35px;padding: 7px" class='glyphicon glyphicon-home'></span>
                </a>
            {% endif %}
            <div id="page-content-wrapper">
                <div class="container-fluid">
            {% block body %}
            {% endblock %}
                </div>
            </div>
        </div>
    </nav>
</div>

{% block javascripts %}
    <script src="https://code.jquery.com/jquery-3.6.0.min.js" integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4=" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-OERcA2EqjJCMA+/3y+gxIOqMEjwtxJY7qPCqsdltbNJuaOe923+mo//f6V8Qbsw3" crossorigin="anonymous"></script>
{% endblock %}
</body>
</html>

